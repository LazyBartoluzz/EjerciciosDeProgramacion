package demo;

public class MainPrueba {

	public static void main(String[] args) {
		

		Integer valor=6356;
		Integer valor2=2;
		
		while (valor > valor2 ) {
			valor*=2;
		}
		
		if (valor==valor2) {
			
			System.out.println("es par");
		} else {
			System.out.println("no es par");
		}
		
	}

}
